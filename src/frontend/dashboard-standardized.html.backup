                    updateDistortionInsights(distortionData.data?.distortion_tenants || []);
                    updateUpsellInsights(upsellData.data?.upsell_opportunities || []);
                } else {
                    console.warn('‚ö†Ô∏è Insights API n√£o dispon√≠vel');
